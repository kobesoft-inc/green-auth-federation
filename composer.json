{
    "name": "kobesoft/green-auth-federation",
    "type": "library",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.3",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0",
        "laravel/socialite": "^5.0",
        "socialiteproviders/microsoft-azure": "^5.0",
        "socialiteproviders/google": "^4.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Green\\Auth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Green\\Auth\\GreenAuthFederationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
